{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "SuperHero.Api": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "SuperHero.Api": {
            "Address": "https://localhost:44394"
          }
        }
      },
      "ClientReact": {
        "Destionation": {
          "ReactClient": {
            "Address": "http://localhost:3000"
          }
        }
      }
    },
    "Routes": {
      "SuperHero-route": {
        "ClusterId": "SuperHero.Api",
        "Match": {
          "Path": "/SuperHero/{**catchall}"
        }
      },
      "Client-route": {
        "ClusterId": "ClientReact",
        "Match": {
          "Path": "{**catchall}"
        }
      }
    }
  }
}
